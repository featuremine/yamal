/**
 * @file version.h
 * @author Ivan Gonzalez
 * @date 24 Feb 2022
 * @brief File contains C declaration of version API of fmc common
 *
 * This file contains C declaration of version API of fmc common.
 * @see http://www.featuremine.com
 */

#ifndef __FMC_VERSION_H__
#define __FMC_VERSION_H__

/**
 * @brief Major FMC version
 * 
 * Major version change represents a change to protocol
 */
#define FMC_VERSION_MAJOR @FMC_VERSION_MAJOR@

/**
 * @brief Minor FMC version
 * 
 * Minor version change represents a change to an API
 */
#define FMC_VERSION_MINOR @FMC_VERSION_MINOR@

/**
 * @brief Patch FMC version
 * 
 * Patch version represents fixes and additons that are
 * backwards compatible with previous API
 */
#define FMC_VERSION_PATCH @FMC_VERSION_PATCH@

/**
 * @brief FMC version string
 *
 * FMC version encoded string "MAJOR.MINOR.PATCH"
 */
#define FMC_VERSION @FMC_VERSION@

#endif // __FMC_VERSION_H__
